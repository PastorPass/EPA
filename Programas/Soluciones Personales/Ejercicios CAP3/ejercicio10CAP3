programa ejercicio10CAP3
areas
  ciudad:AreaC(1,1,100,100)
robots
  robot robot1
  variables
    papeles:numero
  comenzar
    papeles:=0
    repetir 4
      repetir 99
        mientras(HayPapelEnLaEsquina)
          tomarPapel
          papeles:=papeles+1
        mover
      derecha
    Informar(papeles)
  fin
variables
  R-info:robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin
programa CAP5ejemplo5e
procesos
  proceso cuadrado
  comenzar
    repetir 4
      repetir 2
        mover
      derecha
  fin
areas
  ciudad:AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    Pos(2,1)
    repetir 3
      cuadrado
      Pos(PosAv+3,1)
  fin
variables
  R-info:robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin